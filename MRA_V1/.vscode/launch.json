{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/src"
    },
    {
      "name": "Streamlilt",
      "type": "debugpy",
      "request": "launch",
      "module": "streamlit",
      "args": [
        "run",
        "front/main.py"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/src"
    }
  ]
}